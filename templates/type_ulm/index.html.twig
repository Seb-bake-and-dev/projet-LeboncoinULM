{% extends 'base.html.twig' %}

{% block title %}TypeUlm index{% endblock %}

{% block body %}
    <h1>TypeUlm index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_ulm in type_ulms %}
            <tr>
                <td>{{ type_ulm.id }}</td>
                <td>{{ type_ulm.Type }}</td>
                <td>
                    <a href="{{ path('type_ulm_show', {'id': type_ulm.id}) }}">show</a>
                    <a href="{{ path('type_ulm_edit', {'id': type_ulm.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_ulm_new') }}">Create new</a>
{% endblock %}